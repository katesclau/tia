{
  "name": "tia",
  "version": "1.0.0",
  "description": "Tia Node/Express/Mongo test",
  "main": "lib/index.js",
  "scripts": {
    "pretest": "yarn dotenv -e .env.test ./start_db.sh",
    "db:local:start": "yarn dotenv -e .env ./start_db.sh",
    "db:local:stop": "yarn dotenv -e .env ./stop_db.sh",
    "test": "jest",
    "lint": "tslint -c tslint.json --fix 'src/**/*.ts'",
    "dev": "npm run db:local:start && nodemon src/index.ts",
    "build": "tsc",
    "start": "node lib/index.js"
  },
  "keywords": [
    "node",
    "typescript",
    "mongo",
    "express"
  ],
  "author": "Diego Ferreira",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "mongoose": "^5.12.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/supertest": "^2.0.11",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4"
  },
  "engines": {
    "node": ">=14"
  }
}
